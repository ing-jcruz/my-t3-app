generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

model Products {
  id Int @id @default(autoincrement())
  name String
  slug String
  description String
  inventory Int
  price Decimal
}

model ShoppingCart {
  id Int @id @default(autoincrement())
  product_id String
  cantidad Int
  subtotal Decimal
}
